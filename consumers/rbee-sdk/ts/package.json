{
  "name": "@rbee/sdk",
  "version": "0.0.0",
  "private": true,
  "description": "WASM bindings for rbee-sdk (generated via wasm-pack)",
  "type": "module",
  "main": "./pkg/rbee_sdk.js",
  "module": "./pkg/rbee_sdk.js",
  "types": "./pkg/rbee_sdk.d.ts",
  "exports": {
