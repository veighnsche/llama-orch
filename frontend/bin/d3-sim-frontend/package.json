{
    "name": "rbee-d3-sim-frontend",
    "version": "0.0.0",
    "private": true,
    "type": "module",
    "engines": {
        "node": "^20.19.0 || >=22.12.0"
    },
    "prettier": "rbee-frontend-tooling/prettier.config.cjs",
    "scripts": {
        "dev": "vite",
        "build": "run-p type-check \"build-only {@}\" --",
