        "@vitejs/plugin-vue": "^6.0.1",
        "@vue/tsconfig": "^0.7.0",
        "eslint": "^9.14.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-vue": "^9.30.0",
        "npm-run-all2": "^8.0.4",
        "rbee-frontend-tooling": "workspace:*",
        "prettier": "^3.3.3",
        "typescript": "~5.8.0",
        "typescript-eslint": "^8.12.2",
        "vite": "^7.0.6",
        "vite-plugin-vue-devtools": "^8.0.0",
        "vue-eslint-parser": "^9.4.3",
        "vue-tsc": "^3.0.4"
