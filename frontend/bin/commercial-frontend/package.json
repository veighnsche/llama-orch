{
  "name": "orchyra-commercial-frontend-nl",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "prettier": "orchyra-frontend-tooling/prettier.config.cjs",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "npm run build && wrangler dev",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "deploy": "npm run build && wrangler deploy",
    "cf-typegen": "wrangler types",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "i18n:report": "pnpm run i18n:extract && node scripts/i18n-check.mjs --report",
    "i18n:missing": "pnpm run i18n:extract && node scripts/i18n-check.mjs --print-missing",
    "i18n:unused": "pnpm run i18n:extract && node scripts/i18n-check.mjs --print-unused",
    "i18n:unused:strict": "pnpm run i18n:extract && node scripts/i18n-check.mjs --print-unused --fail-on-unused",
    "i18n:extract": "node scripts/i18n-extract-ast.mjs",
    "i18n:migrate": "node scripts/i18n-migrate-from-ts.mjs",
    "i18n:merge": "node scripts/i18n-merge.mjs",
    "i18n:check": "pnpm run i18n:extract && node scripts/i18n-check.mjs",
    "i18n:prune": "pnpm run i18n:extract && node scripts/i18n-prune.mjs --print",
    "i18n:prune:write": "pnpm run i18n:extract && node scripts/i18n-prune.mjs --write --prune-empty-objects"
  },
  "dependencies": {
    "orchyra-storybook": "workspace:*",
    "vue": "^3.5.18",
    "vue-i18n": "^9.14.0",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.13.4",
    "@eslint/js": "^9.14.0",
    "@tsconfig/node22": "^22.0.2",
    "@types/node": "^22.16.5",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-vue": "^9.30.0",
    "json5": "^2.2.3",
    "npm-run-all2": "^8.0.4",
    "orchyra-frontend-tooling": "workspace:*",
    "prettier": "^3.3.3",
    "typescript": "~5.8.0",
    "typescript-eslint": "^8.12.2",
    "vite": "^7.0.6",
    "vite-plugin-vue-devtools": "^8.0.0",
    "vue-eslint-parser": "^9.4.3",
    "vue-tsc": "^3.0.4",
    "wrangler": "^4.39.0"
  }
}
